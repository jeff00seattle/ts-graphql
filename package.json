{
  "name": "ts-graphql",
  "version": "1.0.0",
  "description": "GraphQL and TypeScript",
  "license": "ISC",
  "author": "",
  "homepage": "https://github.com/jeff00seattle/ts-graphql#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeff00seattle/ts-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/jeff00seattle/ts-graphql/issues"
  },
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "prebuild": "npm run code:clean",
    "build": "npm run build:only",
    "build:only": "tsc",
    "code:check": "gts lint",
    "code:clean": "gts clean",
    "code:fix": "gts fix",
    "compile": "tsc",
    "npm:format": "npx prettier-package-json --write package.json",
    "prepare": "husky install",
    "prestart": "npm run compile",
    "start": "npx ts-node ./src/index.ts",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write ."
  },
  "dependencies": {
    "@apollo/server": "^4.0.3",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-format-staged": "^3.0.0",
    "gts": "^3.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "keywords": [],
  "engines": {
    "node": ">=14.20.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
